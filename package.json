{
  "name": "@yog4a/pgsql",
  "description": "Personal encapsulation module for pgsql",
  "version": "0.7.13",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "tsup",
    "build:patch": "pnpm version patch && tsup",
    "build:minor": "pnpm version minor && tsup",
    "build:major": "pnpm version major && tsup",
    "version:patch": "pnpm version patch",
    "version:minor": "pnpm version minor",
    "version:major": "pnpm version major",
    "update": "pnpm up --latest",
    "publish:npm": "pnpm publish --registry https://registry.npmjs.org/"
  },
  "devDependencies": {
    "@types/node": "^24.7.0",
    "@types/pg": "^8.15.5",
    "dotenv": "^16.6.1",
    "tsup": "^8.5.0",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "pg": "^8.16.3",
    "pg-connection-string": "^2.9.1",
    "pg-cursor": "^2.15.3",
    "pg-native": "^3.5.2",
    "pg-pool": "^3.10.1",
    "pg-protocol": "^1.10.3",
    "pg-query-stream": "^4.10.3"
  },
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs",
      "default": "./dist/index.mjs"
    },
    "./clients": {
      "types": "./dist/clients.d.ts",
      "import": "./dist/clients.mjs",
      "require": "./dist/clients.cjs",
      "default": "./dist/clients.mjs"
    },
    "./utils": {
      "types": "./dist/utils.d.ts",
      "import": "./dist/utils.mjs",
      "require": "./dist/utils.cjs",
      "default": "./dist/utils.mjs"
    }
  }
}
