{
  "name": "@yog4a/pgsql",
  "description": "Personal encapsulation module for pgsql",
  "version": "1.1.0",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "scripts": {
    "prepack": "tsup",
    "build": "tsup",
    "build:patch": "npm version patch && npm run build",
    "build:minor": "npm version minor && npm run build",
    "build:major": "npm version major && npm run build",
    "version:patch": "npm version patch",
    "version:minor": "npm version minor",
    "version:major": "npm version major",
    "update": "npx npm-check-updates -u && npm install",
    "update:check": "npx npm-check-updates -u",
    "update:install": "npm install",
    "release": "git push --follow-tags"
  },
  "devDependencies": {
    "@types/node": "^25.2.3",
    "@types/pg": "^8.16.0",
    "dotenv": "^17.3.1",
    "tsup": "^8.5.1",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "pg": "^8.18.0"
  },
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs",
      "default": "./dist/index.mjs"
    },
    "./utils": {
      "types": "./dist/utils.d.ts",
      "import": "./dist/utils.mjs",
      "require": "./dist/utils.cjs",
      "default": "./dist/utils.mjs"
    },
    "./pg": {
      "types": "./dist/packages/pg.d.ts",
      "import": "./dist/packages/pg.mjs",
      "require": "./dist/packages/pg.cjs",
      "default": "./dist/packages/pg.mjs"
    },
    "./clients": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs",
      "default": "./dist/index.mjs"
    }
  }
}
